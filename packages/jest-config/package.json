{
  "name": "@jmlweb/jest-config",
  "version": "0.1.1",
  "description": "Base Jest configuration for jmlweb projects with TypeScript support and coverage settings",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist",
    "README.md",
    "CHANGELOG.md"
  ],
  "scripts": {
    "build": "tsup",
    "clean": "rm -rf dist",
    "prepublishOnly": "node ../../scripts/validate-package.mjs && pnpm build"
  },
  "keywords": [
    "coverage",
    "jest",
    "jest-config",
    "test-config",
    "testing",
    "typescript"
  ],
  "author": "jmlweb",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/jmlweb/tooling.git"
  },
  "bugs": "https://github.com/jmlweb/tooling/issues",
  "homepage": "https://github.com/jmlweb/tooling/tree/main/packages/jest-config#readme",
  "engines": {
    "node": ">=20.11.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "@types/jest": ">=29.0.0",
    "jest": ">=29.0.0",
    "ts-jest": ">=29.0.0"
  },
  "devDependencies": {
    "@jmlweb/tsconfig-internal": "workspace:*",
    "@types/jest": "^29.5.14",
    "jest": "^29.7.0",
    "ts-jest": "^29.4.6",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3"
  }
}
