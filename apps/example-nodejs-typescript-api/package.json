{
  "name": "nodejs-typescript-api-example",
  "version": "1.0.0",
  "description": "Example Node.js TypeScript API using @jmlweb tooling packages",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "tsx src/index.ts",
    "start": "node dist/index.js",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"**/*.{ts,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,json,md}\"",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "express": "^5.2.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@jmlweb/eslint-config-base": "workspace:*",
    "@jmlweb/prettier-config-base": "workspace:*",
    "@jmlweb/tsconfig-base": "workspace:*",
    "@jmlweb/vitest-config": "workspace:*",
    "@types/express": "^5.0.6",
    "@vitest/coverage-v8": "^4.0.16",
    "@vitest/ui": "^4.0.16",
    "eslint": "^9.39.2",
    "prettier": "^3.3.3",
    "tsx": "^4.19.2",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.20.0",
    "vitest": "^4.0.16"
  },
  "prettier": "@jmlweb/prettier-config-base",
  "engines": {
    "node": ">=20.11.0"
  }
}
